#LA 8
class book:
    def __init__(self, title,author):
        self.title = title
        self.author = author
        
x = book("A thousand Sunny", "Ruffy")
y = book("Thousand","The Who")
print(f"{x.title}, {x.author}")
del x.author
#print(f"{x.title}, {x.author}")
print(f"{y.title}, {y.author}")
